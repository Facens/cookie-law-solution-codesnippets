function mycallback() {

/** After dynamic content... **/

var loader = function() {
var s = document.createElement("script"), tag = document.getElementsByTagName("script")[0];
s.src = "https://cdn.iubenda.com/iubenda.js";
tag.parentNode.insertBefore(s, tag);
};

var Alink = $('<a href="https://www.iubenda.com/privacy-policy/88231894" class="iubenda-black iubenda-embed" title="Privacy Policy ">Privacy Policy</a>');
$('#link_container').append(Alink);
loader();
};
